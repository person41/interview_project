<h1>The Logz</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Population</th>
      <th>Ip</th>
    </tr>
  </thead>

  <tbody>
    <% @log_records.each do |log_record| %>
      <tr>
        <td><%= log_record.year %></td>
        <td><%= log_record.population %></td>
        <td><%= log_record.ip %></td>
      </tr>
    <% end %>
  </tbody>
</table>
