<h1>The Logz</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Population</th>
      <th>Population calculation</th>
      <th>Ip</th>
    </tr>
  </thead>

  <tbody>
    <% @log_records.each do |log_record| %>
      <tr>
        <td><%= log_record.year %></td>
        <td><%= log_record.population %></td>
        <td><%= log_record.exact? ? "exact" : "calculated" %></td>
        <td><%= log_record.ip %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>COUNT</th>
    </tr>
  </thead>

  <tbody>
    <% @requests_by_year.each do |req| %>
      <tr>
        <td><%= req.year %></td>
        <td><%= req.request_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
